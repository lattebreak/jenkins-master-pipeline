pipeline {
    agent {
        label 'jenkins-agent1'
    }
    environment {
        DOCKER_HOST = 'tcp://socat-proxy-for-docker-host:2375'
    }
    stages {
        stage('Build Docker agent Image') {
            steps {
                sh 'docker run -v /var/run/docker.sock:/var/run/docker.sock lattebean/jenkins-agent:v1.1'
                sh 'docker version'
            }
        }
    }
}